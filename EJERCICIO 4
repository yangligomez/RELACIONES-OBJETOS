class Animal:
    def hacer_sonido(self):
        return "Sonido gen√©rico"

class Perro(Animal):
    def hacer_sonido(self):
        return "Ladrido"

class Gato(Animal):
    def hacer_sonido(self):
        return "Maullido"

# Crear instancias de Perro y Gato
mi_perro = Perro()
mi_gato = Gato()

# Mostrar los sonidos
print(f"El perro hace: {mi_perro.hacer_sonido()}")
print(f"El gato hace: {mi_gato.hacer_sonido()}")
